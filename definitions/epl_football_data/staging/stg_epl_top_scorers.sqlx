config {
    type: "table",
    database: "transformed-football-data",
    schema: "staging_epl"
}

WITH scorers as (
SELECT
          player_id
        , team_id
        , playedMatches as matches_played
        , goals
        , assists
        , safe_cast(penalties as int64) as penalties
        , last_modified
FROM   ${ref("epl_football_data", "epl_top_scorers")}
)

SELECT *
FROM scorers