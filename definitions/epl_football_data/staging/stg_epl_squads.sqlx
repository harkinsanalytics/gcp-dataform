config {
    type: "table",
    database: "transformed-football-data",
    schema: "staging_epl"
}

WITH t1 as (
SELECT
      team_id
    , team_name
    , player_id
    , player_name
    , player_position
    , player_nationality
FROM ${ref('epl_football_data', 'epl_squad_data')}
)

SELECT *
FROM t1